#!/usr/bin/fish
set out profile.prof
begin
	if test (count $argv) -lt 1
		echo Profile program with given input to $out, then open snakeviz
		echo Usage: (status -f) INPUT
		exit 2
	end
	if test ! -r $argv[1]
		echo $argv[1] is not a file or not readable
		exit 2
	end
end >&2
python -m cProfile -o $out sol.py < $argv[1]
and begin
    echo $out
    snakeviz $out
end
